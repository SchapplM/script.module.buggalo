msgctxt "#90000"
msgid "[B]That's it man, game over man, [I]Game over![/I][/B] ~ Aliens (1986)"
msgstr "[B]Ez van, játék vége ember, [I]Játék vége![/I][/B] ~ Aliens (1986)"

msgctxt "#90001"
msgid "[B]You can't handle the truth![/B] ~ A Few Good Men (1992)"
msgstr "[B]Nem tudod kezelni az igazságot![/B] ~ Egy becsületbeli ügy (1992)"

msgctxt "#90002"
msgid "[B]Danger Will Robinson[/B] ~ Lost in Space (1998)"
msgstr "[B]Baj lesz Robinson[/B] ~ Lost in Space – Elveszve az űrben (1998)"

msgctxt "#90003"
msgid "[B]Why does it cry, Sméagol?[/B] ~ The Two Towers (2002)"
msgstr "[B]Mért sírsz Szmeagol?[/B] ~ A Két Torony (2002)"

msgctxt "#90004"
msgid "[B]Houston, we have a problem...[/B] ~ Apollo 13 (1995)"
msgstr "[B]Houston, van egy kis gondunk...[/B] ~ Apollo 13 (1995)"

msgctxt "#90005"
msgid "[B]E.T. phone home[/B] ~ E.T. (1982)"
msgstr "[B]E.T. haza telefonál[/B] ~ E.T. (1982)"

msgctxt "#90006"
msgid "[B]Frankly, my dear, I don't give a damn[/B] ~ Gone with the Wind (1939)"
msgstr "[B]Őszintén kedvesem, nem is érdekel[/B] ~ Elfújta a Szél (1939)"

msgctxt "#90007"
msgid "[B]Go ahead, make my day[/B] ~ Sudden Impact (1983)"
msgstr "[B]Tűnj el, tedd jobbá a napom[/B] ~ Az igazság útja (1983)"

msgctxt "#90008"
msgid "[B]May the Force be with you[/B] ~ Star Wars (1977)"
msgstr "[B]Az Erő legyen veled![/B] ~ Star Wars (1977)"

msgctxt "#90009"
msgid "[B]You talkin' to me?[/B] ~ Taxi Driver (1976)"
msgstr "[B]Hozzám beszélsz?[/B] ~ Taxisofőr (1976)"

msgctxt "#90010"
msgid "[B]Round up the usual suspects[/B] ~ Casablanca (1942)"
msgstr "[B]A tettes ismeretlen[/B] ~ Casablanca (1942)"

msgctxt "#90011"
msgid "[B]You're gonna need a bigger boat[/B] ~ Jaws (1975)"
msgstr "[B]Szüksége lesz egy nagyobb hajóra[/B] ~ Cápa (1975)"

msgctxt "#91000"
msgid "Unfortunately an error occurred in the [B]%s[/B] addon."
msgstr "Sajnos hiba következett be a [B]%s[/B] kiegészítőben."

msgctxt "#91001"
msgid "You can help with fixing the problem by submitting an error report. "
msgstr "Segíthet a probléma megoldásában azzal, hogy elküldi a hibajelentést."

msgctxt "#91002"
msgid "Click [I]Show error report[/I] for details. No personal information is sent."
msgstr "Kattintson a [I]Hibaüzenet mutatása[/I] feliratra a részletekért. Nem küldünk személyes információkat."

msgctxt "#91003"
msgid "Submit error report"
msgstr "Hibaüzenet küldése"

msgctxt "#91004"
msgid "Don't submit"
msgstr "Ne küldje el"

msgctxt "#91005"
msgid "Thank you for contributing!"
msgstr "Köszönjük segítségét!"

msgctxt "#91006"
msgid "Show error report"
msgstr "Hibaüzenet mutatása"

msgctxt "#91007"
msgid "Hide error report"
msgstr "Hibaüzenet elrejtése"

msgctxt "#91008"
msgid "Unfortunately a problem occurred while showing the details..."
msgstr "Sajnos hiba történt a részletek megmutatása közben"

msgctxt "#91009"
msgid "Unable to send error report!"
msgstr "Nem lehetséges hibaüzenet küldése"

